    "from mathpad import *\n",
    "\n",
    "t = \"t\" * seconds  # \"t seconds\"\n",
    "\n",
    "velocity = 5 * miles / hour  # \"5 miles/hour\"\n",
    "\n",
    "deceleration = 2 * meters / second ** 2  # \"10 meters/second**2\"\n",
    "\n",
    "solution = solve(t == velocity / deceleration, t)  # \"Solution(t = 1.1176 seconds)\"\n",
    "print(solution)\n",
    "\n",
    "solution[t]"
